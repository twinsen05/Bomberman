qmake: FORCE
	@$(QMAKE) -spec linux-g++ CONFIG+=debug -o Makefile Bomberman.pro

qmake_all: FORCE

dist: 
	@test -d .tmp/Bomberman1.0.0 || mkdir -p .tmp/Bomberman1.0.0
	$(COPY_FILE) --parents $(DIST) .tmp/Bomberman1.0.0/ && (cd `dirname .tmp/Bomberman1.0.0` && $(TAR) Bomberman1.0.0.tar Bomberman1.0.0 && $(COMPRESS) Bomberman1.0.0.tar) && $(MOVE) `dirname .tmp/Bomberman1.0.0`/Bomberman1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/Bomberman1.0.0


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


distclean: clean 
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) Makefile


