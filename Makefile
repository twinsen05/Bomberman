####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/opt/Bomberman/bin || mkdir -p $(INSTALL_ROOT)/opt/Bomberman/bin
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/Bomberman/bin/$(QMAKE_TARGET)"

uninstall_target: FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/Bomberman/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/Bomberman/bin/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

